page{
  background-color: #fff;
}
.clock-in-btn{
  position: absolute;
  right: 32rpx;
  top: 32rpx;
}
.line-item-wrap .line-row{
  margin-bottom: 8rpx;
}
.line-item-wrap .line-item-title{
  font-size: 32rpx;
}
.schedule-title{
  padding: 0 40rpx;
  font-size: 28rpx;
  background-color: #fff;
}
.schedule-title>view{
  padding: 22rpx 0;
}
.date-opt-btn{
  font-size: 34rpx;
  color: #3E7EE2;
  padding: 0 32rpx;
}
.date-opt-btn.disabled{
  color: #999999;
}
.select-box{
  padding-right: 50rpx;
}
.select-box::after{
  top: 5rpx;
}
.weui-msg__desc{
  color: #999;
  font-size: 28rpx;
}
.vehicle-num{
  padding-left: 24rpx;
  margin-left: 24rpx;
  border-left: 1px solid #999;
}
.line-item-title .bdicon{
  padding-bottom: 0;
  margin-right: 5rpx;
}
.fix-code{
  width: 120rpx;
  height: 120rpx;
  position: fixed;
  right: 12rpx;
  bottom: 180rpx;
  z-index: 99;
}
.fix-code-active{
  background-color: transparent;
}
.fix-code image{
  width: 100%;
  height: 100%;
}